@isTest
public class UpdateAccountTest{
    @isTest private static void TestContactTrue()
    {
        Account oldAcc=new Account(Name='Riya',First_Name__c='Appu',Last_Name__c='sara');
        insert oldAcc;
    }
    @isTest private static void TestContactFalse()
    {
        Account newAcc=new Account(Name='Rina',First_Name__c='Rohan',Last_Name__c='vyas');
        insert newAcc;
        Contact Con=new Contact(FirstName='Bala',LastName='Kumar',AccountId=newAcc.Id);
        insert Con;
    }
    
    
}
